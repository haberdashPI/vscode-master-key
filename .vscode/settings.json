{
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "yaml",
        "github-actions-workflow" // for GitHub Actions workflow files
    ],
    "rust-analyzer.linkedProjects": [
        "src/rust/parsing/Cargo.toml"
    ],
    "workbench.colorCustomizations": {},
    "window.title": "${activeEditorShort}${separator}${rootName}${separator}${profileName}",
    "projectColors.name": "vscode-master-key",
    "projectColors.mainColor": "#681dd7",
    "projectColors.isActivityBarColored": false,
    "projectColors.isTitleBarColored": false,
    "projectColors.isStatusBarColored": false,
    "projectColors.isProjectNameColored": false,
    "projectColors.isActiveItemsColored": false,
    "projectColors.setWindowTitle": false,
}
